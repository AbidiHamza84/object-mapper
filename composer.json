    "type": "library",
    "keywords": ["object mapper", "data mapper", "map", "dto", "pathfinding"],
    "homepage": "https://github.com/opportus/object-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Cl√©ment Cazaud",
            "email": "clement.cazaud@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "phpbench/phpbench": "^0.16.10 || ^0.17.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Opportus\\ObjectMapper\\": "src/"
        }
    },
    "autoload-dev": {
