{
    "name": "opportus/object-mapper",
    "description": "Allows copying state of objects to differently typed objects using extensible controls and mapping strategies.",
    "type": "library",
    "keywords": ["object mapper", "mapper", "map", "pathfinding", "filter", "recursion", "dto"],
    "homepage": "https://github.com/opportus/object-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Cl√©ment Cazaud",
            "email": "clement.cazaud@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "phpbench/phpbench": "^0.16.10",
        "friendsofphp/php-cs-fixer": "^2.15",
        "dg/bypass-finals": "^1.1"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Opportus\\ObjectMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opportus\\ObjectMapper\\Tests\\": "tests/",
            "Opportus\\ObjectMapper\\Benchmarks\\": "benchmarks/"
        }
    }
}
